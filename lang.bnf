program ::= stmt*
stmt ::= expr ';' | let_stmt | set_stmt
let_stmt ::= 'let' ident '=' expr ';'
set_stmt ::= 'set' ident '=' expr ';'
expr ::= eql
eql ::= rel ('=' rel | '~=' rel)*
rel ::= add ('<' add | '<=' add | '>' add | '>=' add)*
add ::= unary ('+' mul | '-' mul)*
unary ::= '-'? mul
mul ::= primary ('*' primary | '/' primary)*
primary ::= number | ident | '(' expr ')'